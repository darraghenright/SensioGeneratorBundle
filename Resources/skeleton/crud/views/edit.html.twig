{{ "{% extends '::base.html.twig' %}" }}

{{ "{% block stylesheets %}" }}
    {{ '{{ parent() }}' }}
    <link rel="stylesheet" type="text/css" media="all" href="{{ "{{ asset('bundles/sensiogenerator/crud/css/style.css') }}" }}" />
{{ '{% endblock %}' }}

{{ "{% block body %}" }}

    
    
    <h2>{{ entity }} edit</h2>
    
    <form id="{{ entity }}_edit" action="{{ "{{ path('"~ route_name_prefix ~"_update', { 'id': entity.id }) }}" }}" method="post" {{ "{{ form_enctype(edit_form) }}" }}>
        {{ "{{ form_widget(edit_form) }}" }}
        <p>
            <button type="submit">Edit</button>
        </p>
    </form>
    
    {% set hide_edit, hide_delete = true, false %}
    {% include 'views/others/record_actions.html.twig' %}

{{ "{% endblock %}" }}
